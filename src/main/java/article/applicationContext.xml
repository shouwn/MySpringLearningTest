<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="testConnectionMaker" class="article.TestConnectionMaker" />
	<bean id="productionConnectionMaker" class="article.ProductionConnectionMaker" />
	
	<bean id="articleDAO" class="article.DAO.ArticleDAO">
		<property name="connectionMaker" ref="testConnectionMaker" />
	</bean>


</beans>
